{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"noImplicitAny": false,
		"outDir": "out",
		"lib": [
			"es6",
			"dom",
			"es2017"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}