{
  "name": "flutter-fly",
  "displayName": "Flutter Fly: Wireless Debug & Run",
  "description": "The ultimate Flutter development companion for VSCode. Connect devices wirelessly, build APKs/AABs, run Flutter apps, and manage your entire Flutter workflow from one beautiful interface.",
  "version": "1.0.0",
  "publisher": "jamalihassan0307",
  "repository": "https://github.com/jamalihassan0307/flutter-fly",
  "homepage": "https://github.com/jamalihassan0307/flutter-fly",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "media/icon.png",
  "galleryBanner": {
    "color": "#02569B",
    "theme": "dark"
  },
  "categories": [
    "Other",
    "Debuggers",
    "Mobile",
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "flutter",
    "dart",
    "android",
    "ios",
    "mobile",
    "development",
    "wireless",
    "debugging",
    "apk",
    "aab",
    "build",
    "run",
    "hot reload",
    "device management",
    "emulator",
    "simulator"
  ],
  "main": "./out/main.js",
  "test": "jest",
  "scripts": {
    "watch": "tsc -watch",
    "vscode:prepublish": "npm run setup && npm run compile",
    "setup": "npm install",
    "compile": "tsc",
    "publish": "npx vsce publish -p $VSCE_DEPLOY_TOKEN",
    "pp": "npx vsce package && npx vsce publish",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "_____________________________________________________": "UTILITY SCRIPTS",
    "prepare": "husky install",
    "commit": "npm run cm",
    "cm": "cz",
    "changelog": "npx conventional-changelog-cli -p cz-emoji-config -i CHANGELOG.md -s -r 0",
    "cma": "git add . && cz",
    "ri": "npm run release",
    "rici": "release-it --ci",
    "release": "release-it",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "favolosoEmoji": {
    "showEmojiPerCommit": true
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.14",
    "diff": ">=3.5.0",
    "local-devices": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@favoloso/conventional-changelog-emoji": "^0.10.0",
    "@release-it/bumper": "^4.0.0",
    "@types/jest": "^24.9.1",
    "@types/node": "^8.10.66",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "acorn": ">=5.7.4",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "concurrently": "^7.1.0",
    "conventional-changelog-cli": "^3.0.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "conventional-changelog-cz-emoji-config": "^1.0.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^6.3.0",
    "cz-emoji": "^1.3.2-canary.2",
    "diff": ">=3.5.0",
    "eslint": "^8.26.0",
    "husky": "^7.0.0",
    "jest": "^29.2.2",
    "minimist": ">=1.2.6",
    "release-it": "^15.11.0",
    "ts-jest": "^29.0.3",
    "tslint": "^5.20.1",
    "typescript": "4.3",
    "vscode-test": "^1.5.2",
    "yargs-parser": ">=13.1.2"
  },
  "activationEvents": [
    "onCommand:flutterFly.runFlutterDoctor",
    "onCommand:flutterFly.upgradeFlutterSDK",
    "onCommand:flutterFly.getPackages",
    "onCommand:flutterFly.buildAPK",
    "onCommand:flutterFly.buildAppBundle",
    "onCommand:flutterFly.buildIOS",
    "onCommand:flutterFly.buildWeb",
    "onCommand:flutterFly.runOnConnectedDevice",
    "onCommand:flutterFly.runInDebugMode",
    "onCommand:flutterFly.runInProfileMode",
    "onCommand:flutterFly.runInReleaseMode",
    "onCommand:flutterFly.stopRunningApp",
    "onCommand:flutterFly.hotReload",
    "onCommand:flutterFly.hotRestart",
    "onCommand:flutterFly.connectAndroidDevice",
    "onCommand:flutterFly.openAndroidEmulator",
    "onCommand:flutterFly.openIOSSimulator",
    "onCommand:flutterFly.cleanProject",
    "onCommand:flutterFly.injectResources",
    "onCommand:flutterFly.formatCode",
    "onCommand:flutterFly.analyzeProject",
    "onCommand:flutterFly.setCustomADBPath",
    "onCommand:flutterFly.resetCustomADBPath",
    "onCommand:flutterFly.adbResetPorts",
    "onCommand:flutterFly.connectToDeviceFromList",
    "onCommand:flutterFly.adbwificonnect",
    "onCommand:flutterFly.killserver",
    "onCommand:flutterFly.disconnectEverthing",
    "onCommand:flutterFly.enableFirebaseDebug",
    "onCommand:flutterFly.disableFirebaseDebug",
    "onCommand:flutterFly.installAPKFile"
  ],
  "contributes": {
    "commands": [
      {
        "command": "flutterFly.runFlutterDoctor",
        "title": "Flutter Fly:🚀 Run Flutter Doctor",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.upgradeFlutterSDK",
        "title": "Flutter Fly:🚀 Upgrade Flutter SDK",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.getPackages",
        "title": "Flutter Fly:🚀 Get Packages",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.buildAPK",
        "title": "Flutter Fly:🚀 Build APK",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.buildAppBundle",
        "title": "Flutter Fly:🚀 Build App Bundle (AAB)",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.buildIOS",
        "title": "Flutter Fly:🚀 Build iOS",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.buildWeb",
        "title": "Flutter Fly:🚀 Build Web",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.runOnConnectedDevice",
        "title": "Flutter Fly:🚀 Run on Connected Device",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.runInDebugMode",
        "title": "Flutter Fly:🚀 Run in Debug Mode",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.runInProfileMode",
        "title": "Flutter Fly:🚀 Run in Profile Mode",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.runInReleaseMode",
        "title": "Flutter Fly:🚀 Run in Release Mode",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.stopRunningApp",
        "title": "Flutter Fly:🚀 Stop Running App",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.hotReload",
        "title": "Flutter Fly:🚀 Hot Reload",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.hotRestart",
        "title": "Flutter Fly:🚀 Hot Restart",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.connectAndroidDevice",
        "title": "Flutter Fly:🚀 Connect Android Device",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.openAndroidEmulator",
        "title": "Flutter Fly:🚀 Open Android Emulator",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.openIOSSimulator",
        "title": "Flutter Fly:🚀 Open iOS Simulator",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.cleanProject",
        "title": "Flutter Fly:🚀 Clean Project",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.injectResources",
        "title": "Flutter Fly:🚀 Inject Resources",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.formatCode",
        "title": "Flutter Fly:🚀 Format Code",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.analyzeProject",
        "title": "Flutter Fly:🚀 Analyze Project",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.setCustomADBPath",
        "title": "Flutter Fly:📱 Setup custom ADB location",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.resetCustomADBPath",
        "title": "Flutter Fly:📱 Remove custom ADB location",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.adbResetPorts",
        "title": "Flutter Fly:📱 Reset connected devices port",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.connectToDeviceFromList",
        "title": "Flutter Fly:📱 Connect to device from List",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.adbwificonnect",
        "title": "Flutter Fly:📱 Connect to device IP",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.disconnectEverthing",
        "title": "Flutter Fly:📱 Disconnect from any devices",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.installAPKFile",
        "title": "Flutter Fly:📱 Pick .APK file and install",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.killserver",
        "title": "Flutter Fly:⚠️ Kill ADB server",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.enableFirebaseDebug",
        "title": "Flutter Fly:🔥 Enable Firebase events debug mode",
        "category": "Flutter Fly"
      },
      {
        "command": "flutterFly.disableFirebaseDebug",
        "title": "Flutter Fly:🔥 Disable Firebase events debug mode",
        "category": "Flutter Fly"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "flutterFly.runFlutterDoctor",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.upgradeFlutterSDK",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.getPackages",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.buildAPK",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.buildAppBundle",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.runOnConnectedDevice",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.hotReload",
          "when": "workspaceHasFlutterProject"
        },
        {
          "command": "flutterFly.hotRestart",
          "when": "workspaceHasFlutterProject"
        }
      ]
    },
    "configuration": {
      "title": "Flutter Fly",
      "properties": {
        "flutterFly.adbPath": {
          "type": "string",
          "default": "",
          "description": "Custom path to ADB executable"
        },
        "flutterFly.flutterPath": {
          "type": "string",
          "default": "",
          "description": "Custom path to Flutter executable"
        },
        "flutterFly.defaultBuildMode": {
          "type": "string",
          "enum": ["debug", "profile", "release"],
          "default": "debug",
          "description": "Default build mode for APK/AAB builds"
        }
      }
    }
  }
}
