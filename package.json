{
  "name": "flutter-fly",
  "displayName": "Flutter Fly: Wireless Debug & Run",
  "description": "The ultimate Flutter development companion for VSCode. Connect devices wirelessly, build APKs/AABs, run Flutter apps, and manage your entire Flutter workflow from one beautiful interface.",
  "version": "1.0.2",
  "publisher": "jamalihassan0307",
  "repository": "https://github.com/jamalihassan0307/flutter-fly",
  "homepage": "https://github.com/jamalihassan0307/flutter-fly",
  "engines": {
    "vscode": "^1.103.0"
  },
  "icon": "media/icon.png",
  "galleryBanner": {
    "color": "#02569B",
    "theme": "dark"
  },
  "categories": [
    "Other",
    "Debuggers",
    "Programming Languages",
    "Snippets",
    "Extension Packs",
    "Formatters",
    "Linters"
  ],
  "keywords": [
    "flutter",
    "dart",
    "android",
    "ios",
    "mobile-development",
    "wireless-debugging",
    "debugging",
    "apk",
    "aab",
    "build-tools",
    "hot-reload",
    "device-management",
    "emulator",
    "simulator",
    "flutter-doctor",
    "flutter-build"
  ],
  "main": "./out/main.js",
  "scripts": {
    "vscode:prepublish": "npm run setup && npm run compile",
    "setup": "npm install",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "npm run clean && npm run compile && npm run package",
    "clean": "rimraf out dist",
    "package": "vsce package"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "dependencies": {
    "adm-zip": "^0.4.14",
    "diff": ">=3.5.0",
    "local-devices": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.103.0",
    "@vscode/vsce": "^2.32.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  },
  "activationEvents": [
    "onCommand:flutterFly.openFlutterPanel",
    "onCommand:flutterFly.connectWirelessDevice",
    "onCommand:flutterFly.showConnectedDevices",
    "onCommand:flutterFly.runFlutterApp",
    "onCommand:flutterFly.buildFlutterApp",
    "onCommand:flutterFly.runFlutterDoctor",
    "onCommand:flutterFly.upgradeFlutterSDK",
    "onCommand:flutterFly.getPackages",
    "onCommand:flutterFly.buildAPK",
    "onCommand:flutterFly.buildAppBundle",
    "onCommand:flutterFly.buildIOS",
    "onCommand:flutterFly.buildWeb",
    "onCommand:flutterFly.runOnConnectedDevice",
    "onCommand:flutterFly.runInDebugMode",
    "onCommand:flutterFly.runInProfileMode",
    "onCommand:flutterFly.runInReleaseMode",
    "onCommand:flutterFly.stopRunningApp",
    "onCommand:flutterFly.hotReload",
    "onCommand:flutterFly.hotRestart",
    "onCommand:flutterFly.connectAndroidDevice",
    "onCommand:flutterFly.openAndroidEmulator",
    "onCommand:flutterFly.openIOSSimulator",
    "onCommand:flutterFly.cleanProject",
    "onCommand:flutterFly.injectResources",
    "onCommand:flutterFly.formatCode",
    "onCommand:flutterFly.analyzeProject",
    "onCommand:flutterFly.setCustomADBPath",
    "onCommand:flutterFly.resetCustomADBPath",
    "onCommand:flutterFly.adbResetPorts",
    "onCommand:flutterFly.connectToDeviceFromList",
    "onCommand:flutterFly.adbwificonnect",
    "onCommand:flutterFly.killserver",
    "onCommand:flutterFly.disconnectEverthing",
    "onCommand:flutterFly.enableFirebaseDebug",
    "onCommand:flutterFly.disableFirebaseDebug",
    "onCommand:flutterFly.installAPKFile"
  ],
  "contributes": {
    "contexts": [
      {
        "id": "workspaceHasFlutterProject",
        "description": "True when the workspace contains a Flutter project",
        "default": false
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "flutter-fly-sidebar",
          "title": "Flutter Fly",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
